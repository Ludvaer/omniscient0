<% provide(:title, t('Pick word in sets')) %>
<% provide(:assets, javascript_include_tag('pick_word_in_sets')) %>

<div id="pick_word_in_sets">
  <% @pick_word_in_sets.each do |pick_word_in_set| %>
    <% test_render = render pick_word_in_set %>
    <p>
      <%= link_to test_render, pick_word_in_set %>
    </p>
  <% end %>
</div>

<%= render "form", pick_word_in_set: PickWordInSet.new %>
