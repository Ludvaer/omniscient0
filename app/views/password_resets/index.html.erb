<p style="color: green"><%= notice %></p>

<h1>Password resets</h1>

<div id="password_resets">
  <% @password_resets.each do |password_reset| %>
    <%= render password_reset %>
    <p>
      <%= link_to "Show this password reset", password_reset %>
    </p>
  <% end %>
</div>

<%= link_to "New password reset", new_password_reset_path %>
